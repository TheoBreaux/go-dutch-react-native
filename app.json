{
  "expo": {
    "name": "godutch",
    "slug": "godutch",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "backgroundColor": "white",
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Go Dutch! to access your camera."
        }
      ]
    ]
  }
}
